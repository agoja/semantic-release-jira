  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.20",
    "@types/signale": "^1.4.7",
    "jest": "^29.7.0",
    "madge": "^6.1.0",
    "semantic-release": "^22.0.12",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "semantic-release": ">=19.0.0"
  },
  "engines": {
    "node": ">=14.17"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "resolutions": {
